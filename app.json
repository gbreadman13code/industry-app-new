{
  "expo": {
    "name": "Индустрия",
    "slug": "industry-app",
    "version": "1.1.10",
    "orientation": "portrait",
    "icon": "./assets/playstore.png",
    "userInterfaceStyle": "light",
    "scheme": "industry-app",
    "splash": {
      "image": "./assets/playstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow to access your microphone."
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      },
      "bundleIdentifier": "com.ITEO.industryApp",
      "buildNumber": "1.1.10"
    },
    "android": {
      "package": "com.ITEO.industryApp",
      "versionCode": 2,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "MEDIA_LIBRARY",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/playstore.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/playstore.png"
    },
    "extra": {
      "eas": {
        "projectId": "f2ab86bd-7f9f-4fe9-bb0a-553c41e52e92"
      }
    }
  }
}
